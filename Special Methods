print(' Hospital') class Hospital: def init(self,name,age,diseases,city): self.name=name self.age=age self.diseases=diseases self.city=city #__str__method def str(self): patient=f"Name:{self.name}\nAge:{self.age}\nSuffering from:{self.diseases}\nPlace:{self.city}" return patient #repr method def repr(self): patient= f"Hospital(Name:{self.name}\nAge:{self.age}\nSuffering from:{self.diseases}\nPlace:{self.city})" return patient

p=Hospital("Karan","60","chicken pox","Madurai") print(p)

print([p])

